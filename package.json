{
  "name": "money-compass",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "check-styles": "prettier -c \"**/*.{js,ts,jsx,tsx,json,css,md}\"",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "validate": "npm-run-all --parallel check-types && lint-staged",
    "prepare": "husky",
    "test": "echo 'No tests.'",
    "supa:start": "supabase start",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "supabase migration up && supabase gen types --schema=public --local > src/lib/types/database.types.ts",
    "db:migrate:remote": "supabase db push",
    "db:backup": "mkdir -p backups && supabase db dump --local --data-only --file \"backups/backup_local_$(date +%Y%m%d_%H%M%S).sql\"",
    "db:backup:remote": "mkdir -p backups && supabase db dump --linked --data-only --file \"backups/backup_remote_$(date +%Y%m%d_%H%M%S).sql\"",
    "db:reset": "supabase db reset"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@next-safe-action/adapter-react-hook-form": "^2.0.0",
    "@number-flow/react": "^0.5.10",
    "@radix-ui/react-icons": "^1.3.2",
    "@remixicon/react": "^4.6.0",
    "@standard-schema/spec": "^1.0.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.51.0",
    "@tanstack/react-table": "^8.21.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.44.3",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.6",
    "next": "^15.4.2-canary.0",
    "next-safe-action": "^8.0.8",
    "next-themes": "^0.4.6",
    "nordigen-node": "patch:nordigen-node@npm%3A1.4.0#~/.yarn/patches/nordigen-node-npm-1.4.0-c527fb4a7b.patch",
    "nprogress": "^0.2.0",
    "postgres": "^3.4.7",
    "radix-ui": "^1.4.2",
    "react": "19.1.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.60.0",
    "react-number-format": "^5.4.4",
    "react-resizable-panels": "^3.0.3",
    "react-responsive": "^10.0.1",
    "recharts": "^2.15.4",
    "server-only": "^0.0.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@next/eslint-plugin-next": "^15.4.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^24.0.14",
    "@types/nprogress": "^0.2.3",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
